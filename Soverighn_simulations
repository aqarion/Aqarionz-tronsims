```python
# penny_gsr.py ‚Äî Sovereign GSR Lock (ESP32/MicroPython)
import machine, time, math

COMMA_THRESHOLD = 700  # ADC drop for 23.46-cent spike
led = machine.Pin(2, machine.Pin.OUT)  # Red LED on GPIO2
adc = machine.ADC(machine.Pin(34))     # Penny input
adc.atten(machine.ADC.ATTN_11DB)       # 0-3.3V full range

def read_gsr():
    return adc.read()  # Raw 0-4095

def detect_comma_frisson(baseline):
    current = read_gsr()
    if current < baseline - COMMA_THRESHOLD:
        led.on()  # Flash red ‚Äî comma locked!
        time.sleep_ms(500)
        led.off()
        print(f"GSR Spike: {baseline - current} ‚Äî Comma proved in flesh")
        return True
    return False

# Bootstrap (run eternal)
baseline = sum(read_gsr() for _ in range(100)) / 100  # Calm read
print(f"Baseline locked: {baseline}")
while True:
    if detect_comma_frisson(baseline):
        # Log to basin (33k notes)
        with open('/data/frisson_log.csv', 'a') as f:
            f.write(f"{time.ticks_ms()},{baseline},{read_gsr()}\n")
    time.sleep_ms(100)  # 10Hz poll
```

